Section2("Island Of Move", "", function()
    local Codes = {"WorldAlive","DIY","SettingTheStage","StrikeAPose","GetMoving","VictoryLap","ThingsGoBoom","FXArtist","Boardwalk","ParticleWizard","Glimmer"}
for i = 1,#Codes do
    game:GetService("ReplicatedStorage").Events.RedeemCode:FireServer(Codes[i])
end
end)

Section2:NewButton("Galactic Speedway Creator Challenge", "", function()
    local Codes = {"Lesson1","Lesson2","Lesson3","Lesson4","Lesson5","Lesson6"}
